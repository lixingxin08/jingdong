/* pages/detail/detail.wxss */
page{
    height: 100%;
    box-sizing: border-box;
    padding-bottom: 198rpx;
}
.main{
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 100%;
    background-color: #f1f1f1;
}
@import "../../app.wxss";
.detail_header{
    /* background-image: url(data:image/png;base64,); */
    background-color: #ffcd46;
    width: 100%;
    /* 355 */
    height:298rpx;
    background-size: 750rpx 350rpx;
    position: relative;
    flex: none;
}
.logo{
    width:74rpx;
    height: 72rpx;
    display: block;
    margin: 16rpx auto;
}
.shoc{
    width: 36rpx;
    height: 32rpx;
    position: absolute;
    top:20rpx;
    right: 20rpx;
}
.search{
    position: absolute;
    width: 630rpx;
    bottom:10rpx;   
}
.search .search_input{
    background-color: #ffffff;
    line-height: 48rpx; 
    font-size: 20rpx;
    color: #808080;
}
.search icon{
    right: -30rpx;
}
/* 商铺介绍 */
.header_shop{
    width: 100%;
    position: absolute;
    top:100rpx;
    display: flex;
    flex-direction: column ;
    align-items: center;
    justify-content: center;
}
.header_shop text{
    font-size: 24rpx;
    font-weight: 600;
    color: #fff;
}
.header_shop_announcement{
    padding-top: 6rpx;
    margin-left: -200rpx;
}
.header_assess{
    position: absolute;
    right: 20rpx;
    top: 117rpx;
}
.header_assess image{
    width: 34rpx;
    height: 24rpx;
}
.header_assess text{
    font-size: 20rpx;
    color: #fff;
}
/* 滑块 */
.detail_body{
    width: 100%;
    flex: 1 1 auto;
    height: 100%;
     /* overflow-y: auto;  */
    display: flex;
}
.detail_body_left_left{
    background-color: #e5e5e5;
}
.classList{
    height: 80rpx;
    font-size: 24rpx;
    line-height: 80rpx;
    padding-left: 20rpx;
     /* border-top: 1px solid #8a8a8a;  */
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}  
.classList:first-child{
    border-top: none;
}

/* 右边滑块 */
.detail_body_right{
    display: flex;
    flex: 1;
}
.goods{
    box-sizing: border-box;
    display: flex;
    justify-content:space-around;
    flex-flow: wrap;
    padding:0 10rpx;
}
.goodsList{
    position: relative;
    /* height: 292rpx; */
    width: 280rpx;
    margin-top: 20rpx;
    background-color: #fff;
}
.goodsList .goods_img{
    width: 282rpx;
    height: 202rpx;
}
.goodsList_san{
    position: absolute;
    height: 86rpx;
    width: 86rpx;
}
.goodsList .goodsList_goods{
    width: 240rpx;
    height: 160rpx;
    position: absolute;
    top:24rpx;
    left:20rpx;
}
.goodsList_add,.goodsList_jian{
    height:40rpx;
    /* position: absolute;  */
    float: right;
    margin-top: 16rpx;
    margin-bottom: 10rpx;
    margin-right: 10rpx;
    right: 36rpx;
    width: 40rpx;
}
.goodsList_text{
    float: right;
    margin-top: 16rpx;
    height: 36rpx;
    width:36rpx;
    font-size: 28rpx;
    text-align: center;
    color: #ffcd46;
}
.goods_class{
    position: absolute;
    top: 10rpx;
    left: 10rpx;
    font-size: 20rpx;
    color: #fff;
    font-weight: 800;
}
/* .goods_detail{
    display: flex;
    align-items:center; 
} */
.goods_name,.goods_sellPrice{
    font-size: 28rpx;
    padding-left: 20rpx;
    color: #404040;
    font-weight: 700;
}
.goods_content{
    font-size: 20rpx;
    color: #404040; 
    font-weight: 600;  
}
.goods_price{
    float: left;
    margin-top: 16rpx;
   color: #ffcd46;
}
.goods_price .goods_sellPrice{
       color: #ffcd46;
}
.on{
    background-color: #fff;
}
.goods-pay{
    background-color: #ffcd46;
}
/* 购物 */
.goodsPay{
    height:98rpx;
    width: 100%;
    position: fixed;
    bottom:90rpx;
    border-top: 10rpx solid  #f1f1f1;
}
.goods-car{
    position: relative;
    float: left;
    width: 50%;
    height: 98rpx;
    background-color: #4d4d4d;
}
.goods-car image{
    position: absolute;
    bottom: 26rpx;
    left:18rpx;
    width: 46rpx;
    height: 44rpx;
}
.goods-car text{
    padding-right: 56rpx;
    display: block;
    height: 98rpx;
    line-height: 98rpx;
    color: #fff;
    text-align: right;
    font-size: 32rpx; 
    font-weight: 600;
}
.goods-pay{
    float: right;  
    width: 50%;
    height: 98rpx;
    background-color: #ffcd46;
}
.goods-pay text{
    display: block;
    height: 98rpx;
    line-height: 98rpx;
    color: #fff;
    text-align: center;
    font-size: 32rpx;
    font-weight: 600;    
}
.goods-car .goods-car-num{
    position: absolute;
    top: -18rpx;
    left: 66rpx;
    color: #ffcd46;
    font-weight: 500;
}
/* 购物车列表 */
.selectGoods{
    position: fixed;
    bottom: 198rpx;
    width: 710rpx;
    left: 20rpx;
    background-color: #ffffff;
    border-radius: 20rpx;
}
.selectGoods-header{
    box-sizing: border-box;
    height: 64rpx;
    border-bottom: 1px solid #e5e5e5;
}
.selectGoods-header-left{
    padding-left: 72rpx;
}
.selectGoods-header-left icon{
    position: absolute;
    top: 16rpx;
    left: 20rpx;
}
.selectGoods-header text{
   height: 64rpx;
   line-height: 64rpx;
}
.selectGoods-header-right{
    padding-right: 20rpx;
    color: #4d4d4d;
}
.selectGoods-header-right image{
    width: 28rpx;
    height: 28rpx;
    position: absolute;
    top: 20rpx;
    right: 160rpx;
}
.selectGoods-content-list{
    height: 80rpx;
    border-bottom: 1px solid #e5e5e5;
}
.selectGoods-content-list-left{
    height: 80rpx;
}
.selectGoods-content-list-left image{
    height: 60rpx;
    width: 60rpx;
    margin:10rpx;
}
.selectGoods-content-list-left text{
    vertical-align: top;
    display: inline-block;
    line-height: 80rpx;
    font-size: 26rpx;
    color: #4d4d4d;
}
.selectGoods-footer{
    height: 64rpx;
}
.selectGoods-footer-bottom{
    position: absolute;
    bottom: -28rpx;
    left: 30rpx;
    height: 0rpx;
    width: 0rpx;
    border-top:15rpx solid #fff;
    border-bottom:15rpx solid rgba(77, 77, 77,0);
    border-right:15rpx solid rgba(77, 77, 77,0);
    border-left:15rpx solid rgba(77, 77, 77,0);
}